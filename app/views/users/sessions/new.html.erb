              Sign up
            </h4>
          </div>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="modal-body">  
    <div class="row clearfix">
    <div class="col-md-6 column">
      <div class="form-group">
       <%= f.label(:first_name, "First Name")%><%= f.text_field(:first_name, class: 'form-control') %>
      </div>
      <div class="form-group">
        <%= f.label(:last_name, "Last Name")%><%= f.text_field(:last_name, class: 'form-control') %>
      </div>                                    
      <div class="form-group">
         <%= f.label(:email, "Email address")%><%= f.email_field(:email, class: 'form-control') %>
      </div>    
      <div class="form-group">
         <%= f.label(:birthdate, "Birthdate")%><br><%= f.date_field(:birthdate, class: 'form-control') %>
      </div>
      <%= hidden_field_tag(:points, "100") %>           
    </div>
    <div class="col-md-6 column">
      <%= f.label(:gender, "Gender")%>
      <div class="form-group" data-toggle="buttons">
          <label class="btn btn-default">
              <%= radio_button_tag(:gender, "male") %>
          male </label>
          <label class="btn btn-default">
              <%= radio_button_tag(:gender, "female") %>
          female </label>
      </div>

      <div class="form-group">
         <%= f.label(:encrypted_password, "Password")%><%= f.password_field(:encrypted_password, class: 'form-control') %>
      </div>
      <div class="form-group">
         <%= f.label(:repeat_password, "Repeat Password")%><%= f.password_field(:repeat_password, class: 'form-control') %>
      </div>
      <div class="form-group">
        <%= f.label(:avatar, "Choose a photo")%><%= f.file_field :avatar %>       
      </div>
      <div class="checkbox">
         <label><%= check_box_tag(:remember) %>Remember me</label>
      </div>
    </div>    
    </div>                                
  </div>                    
  <div class="modal-footer">
    <button class="btn btn-default" data-dismiss="modal">Close</button>  <%= f.submit "Sign up", class:'btn btn-primary' %>
  </div>
<% end %>

